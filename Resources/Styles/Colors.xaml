<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">


    <Color x:Key="Primary">White</Color>
    <Color x:Key="PrimaryText">#452A03</Color>
    <Color x:Key="Secondary">#0000FF</Color>
    <Color x:Key="SecondaryText">White</Color>

    <Color x:Key="Tertiary">#C8C8C8</Color>
    <Color x:Key="TertiaryText">#141414</Color>
    <Color x:Key="Quaternary">#212121</Color>
    <Color x:Key="QuaternaryText">#ACACAC</Color>
    <Color x:Key="Quinary">Transparent</Color>
    <Color x:Key="White">White</Color>
    <Color x:Key="Black">Black</Color>

    <Color x:Key="EntryBorderColor">Black</Color>
    <Color x:Key="ButtonSelectedColor">LightBlue</Color>
    <Color x:Key="ButtonBorderColor">DarkBlue</Color>
    <Color x:Key="BorderColorHalfTransparenet">#65000000</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource Secondary}"/>
    <SolidColorBrush x:Key="TertiaryBrush" Color="{StaticResource Tertiary}"/>
</ResourceDictionary>